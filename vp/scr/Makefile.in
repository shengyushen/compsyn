	sed 's/\\\([A-Za-z0-9_]*\)\[\([0-9]*\)\] /\1_\2 /g' dc_synres_dual_blif.v > dc_synres_dual_blif2.v
	lsi10k2blif.exe dc_synres_dual_blif2.v resulting_dual

 

